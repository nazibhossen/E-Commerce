{
    "builds": [
        {
            "src": "ecommerce /wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdasize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": " / (.*)",
            "dest": "ecommerce/wsgi.py"
        }
    ]
}